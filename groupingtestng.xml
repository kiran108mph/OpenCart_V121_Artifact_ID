<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Suite">
	<groups>
		<run>
			<include name="common"></include>
			<include name="regression"></include>
			<include name="sanity"></include>
			<!-- <exclude name="sanity"></exclude> -->
		</run>
	</groups>
	
	<listeners>
		<listener class-name="testUtilities.ExtentReportManager"></listener>
	</listeners>

	<test thread-count="5" name="Test">
		<parameter name="os" value="windows"></parameter>
		<parameter name="browser" value="chrome"></parameter>
		<classes>
			<class name="testCases.TC001_AccountRegistration_Test" />
			<class name="testCases.TC002_LogInToMyAccount" />
			<!-- <class name="testCases.TC003_LoginToMyAccountDDT" /> -->
		</classes>
	</test> <!-- Test -->
</suite> <!-- Suite -->
